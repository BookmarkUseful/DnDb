<div class="artwork-fade head-artwork">
  <%= image_tag("source_artwork1.png", :class => "artwork") %>
</div>

<div class="top">
  <div class="main-header-container">
    <%= link_to sources_path, :class => 'top-link' do %>
      <%= image_tag("source_icon.png", :class => "top-icon source-icon") %>
      <h1 class="top-header">Sources</h1>
    <% end %>
  </div>
</div>

<div id="index-container">
  <table class="index">
    <tr style="background-color: white">
      <th>Name</th>
      <th>Author</th>
      <th>Kind</th>
    </tr>
    <%= render :partial => 'source', :collection => @sources, :as => :source %>
  </table>
</div>
